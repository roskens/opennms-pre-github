<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	   .outlineBorder{
	   	   border: 1px solid #B5B8C8;
	   	}
	</ui:style>
    <b:Row>
        <b:Column size="MD_12">
            <b:Form type="INLINE">
                <b:FieldSet>
                    <b:FormGroup>
                        <b:TextBox ui:field="m_textBox"></b:TextBox>
                        <b:Button ui:field="m_goButton" addStyleNames="space-on-the-left">Search</b:Button>
                    </b:FormGroup>
                </b:FieldSet>
            </b:Form>
        </b:Column>
    </b:Row>

</ui:UiBinder>
