<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
    <b:Row >
        <b:Column size="MD_12">
            <b:Form type="INLINE">
                <b:FieldSet>
                    <b:FormGroup>
                        <b:TextBox ui:field="m_textBox" />
                        <b:Button ui:field="m_searchButton" addStyleNames="space-on-the-left">Search</b:Button>
                    </b:FormGroup>
                </b:FieldSet>
            </b:Form>
        </b:Column>
    </b:Row>

</ui:UiBinder>
